<div class="mt-4">
    <div class="row">

        {{#each users}}
        <div class="col-sm-6 col-lg-4">
            <div class="card">
                <a href="/users/{{this.slug}}">
                    <img src="{{this.avatar}}" alt="" class="card-img-top">
                </a>
                <div class="card-body">
                    <a href="/users/{{this.slug}}">
                        <h5 class="card-title">{{this.name}}</h5>
                    </a>
                    <p class="card-text">Tên: {{this.name}} </p>
                    <p class="card-text">Tuổi: {{this.age}} </p>
                    <p class="card-text">Giới tính: {{this.gender}} </p>
                    <a href="#" class="btn btn-primary">Go</a>
                </div>
            </div>
        </div>

        {{/each}}
    </div>
</div>